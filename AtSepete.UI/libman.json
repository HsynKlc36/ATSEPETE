{
  //clientside tarafını yani wwwroot daki framework'leri yönettiğimiz config dosyası!
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.2.2",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "library": "limonte-sweetalert2@11.4.37",
      "destination": "wwwroot/limonte-sweetalert2/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    }
  ]
}